@startuml
class MenuItem {
    -name: str
    -price: int
    --
    + __str__(): str
    + to_dict(): dict
}

class Order {
    -customer_name: str
    -table_number: int
    -item: List<MenuItem>
    -timestamp: str
    --
    + __str__(): str
    + to_dict(): dict
    + total_price(): int
}

class User{
    -name: str
    +login(): bool
}

class Admin {
    + manage_manu(): void
    + generate_statistics(): void
    + manage_orders(): void
}

class Waiter {
    + take_order(): void
    +list_orders(): void
}

class Guest {
    + provide_feedback(): void
}

User <|-- Admin
User <|-- Waiter
User <|-- Guest
@enduml